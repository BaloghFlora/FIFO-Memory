  	Data : in std_logic_VECTOR(3 DOWNTO 0);
  	Direction: in std_logic;
  	Reset: in std_logic;
  	Output: out std_logic_VECTOR(3 downto 0);
  	CLOCK: IN std_logic;
  	LOAD: IN STD_LOGIC
  	
    );
