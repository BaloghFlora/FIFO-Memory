signal temp: std_logic_VECTOR(3 DOWNTO 0);
BEGIN

process(Clock,Reset)
begin
if Reset='1' then
temp &lt;= "0000";
elsif ( Clock'event and Clock='1') then
if Load='1' then
temp &lt;= Data;
elsif (Load='0' and Direction='0') then
temp &lt;= std_logic_vector(unsigned(temp) + 1);
elsif (Load='0' and Direction='1') then
temp &lt;= std_logic_vector(unsigned(temp) - 1);
end if;
end if;
end process;
Output &lt;= temp;
Output&lt;= temp;
	

