LIBRARY ieee;&#13;
USE ieee.std_logic_1164.all;&#13;
&#13;USE ieee.std_logic_unsigned.all; 
ENTITY display_unit IS&#13;
  PORT (&#13;
  data1, data2, data3,data4,data5,data6,data7,data8 : in std_logic_vector(6 downto 0);
  clk, reset: in std_logic;
  anodes: out std_logic_vector(7 downto 0);
  cathodes: out std_logic_vector(6 downto 0)
    );&#13;
END display_unit;&#13;
&#13;
ARCHITECTURE TypeArchitecture OF DISPLAY_UNIT IS

signal new_clk: std_logic;
signal an: std_logic_vector(7 downto 0);

BEGIN

Frequency_divider: process(clk,reset)
	variable var: std_logic_vector(18 downto 0):=(others=&gt;'0');
	begin
		if reset='1' then
			var:=(others=&gt;'0');
		elsif clk'event and clk='1' then
			var:=var+1;
		end if;
	new_clk&lt;=var(18);
	end process Frequency_divider;

Ring_Register_Anodes: process(new_clk)
	variable v: std_logic_vector(7 downto 0):="10000000";
	variable aux: std_logic;
	begin
	if new_clk'event and new_clk='1' then
		aux:=v(0);
		for i in 0 to 6 loop
			v(i):=v(i+1);
		end loop;
		v(7):=aux;
	end if;
	anodes&lt;=v;
	an&lt;=v;
	end process Ring_Register_Anodes;

Display: process(an,reset)
	variable x: std_logic_vector(6 downto 0);
	begin
	if reset='1' then
		case an is
			when "10000000" =&gt; x:="0000000";
			when "01000000" =&gt; x:="0000000";
			when "00100000" =&gt; x:="0000000";
			when others =&gt; x:="0000000";
		end case;
	else
		case an is
			when "10000000" =&gt; x:=data1;
			when "01000000" =&gt; x:=data2;
			when "00100000" =&gt; x:=data3;
			when "00010000" =&gt; x:=data4;
			when "00001000" =&gt; x:=data5;
			when "00000100" =&gt; x:=data6;
			when "00000010" =&gt; x:=data7;
			when "00000001" =&gt; x:=data8;
			when others =&gt; x:="0000000";
		end case;
	end if;
	cathodes&lt;=x;

	end process Display;

END TypeArchitecture;</vhdl>
