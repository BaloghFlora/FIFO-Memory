				else nextstate &lt;= st16;
				end if;
			when st16 =&gt; FULL &lt;= '1';
				if POP = '1' then nextstate &lt;= st15;
				elsif PUSH = '0' then nextstate &lt;= st2;
				else nextstate &lt;= st1;
				end if;
			end case;
			end process transition;


END ARCH;
