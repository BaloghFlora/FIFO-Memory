  <vhdl name="DEBOUNCER">
LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY DEBOUNCER IS
 	PORT (
BUTON,CLK: in std_logic;
 		Y: out std_logic);	
END DEBOUNCER;


ARCHITECTURE struct OF DEBOUNCER IS

COMPONENT DFF
	PORT (CLK,D,S,R: in std_logic;
  		Q: out std_logic);

END COMPONENT;

COMPONENT INP3_AND_GATE
	PORT(X1,X2,X3: in std_logic;
		Y: out std_logic);
END COMPONENT;

signal N1,N2,N3: std_logic;

BEGIN
C1: DFF port map (CLK,BUTON,'1','1',N1);
C2: DFF port map (CLK,N1,'1','1',N2);
C3: DFF port map (CLK,N2,'1','1',N3);
C4: INP3_AND_GATE port map (N1,N2,N3,Y);

END struct;

LIBRARY ieee;
USE ieee.std_logic_1164.all;
ENTITY DFF IS
	PORT (CLK,D,S,R: in std_logic;
  		Q: out std_logic);
END DFF;

ARCHITECTURE A OF DFF IS
BEGIN
D_Flip_Flop: process (CLK)
begin
	if CLK'EVENT and CLK='1' then
		if S='0'then
			Q&lt;='1';
		else
			if R='0' then
				Q&lt;='0';
			else
				Q&lt;=D;
			end if;
		end if;
	end if;	
	
end process D_Flip_Flop;
END A;


LIBRARY ieee;
USE ieee.std_logic_1164.all;
ENTITY INP3_AND_GATE IS
	PORT(X1,X2,X3: in std_logic;
		Y: out std_logic);
END INP3_AND_GATE;

ARCHITECTURE behav OF INP3_AND_GATE IS
BEGIN
	Y&lt;=X1 and X2 and X3;
END behav;
