ARCHITECTURE Behaviorial OF Ram_Proj IS
type MATRIX is array (0 to 15) of std_logic_vector(31 downto 0);
signal Mem: MATRIX := ( x"00000000",x"00000000",x"00000000",x"00000000",x"00000000", x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000",x"00000000");

BEGIN
 --Mem&lt;=c;
 
process (CLOCK, CS_ram, A_ram, WE)
	begin
		if CLock = '1' and CLock'EVENT and CS_ram = '1' then
			if (WE = '1') then
				Mem (conv_integer(A_ram)) &lt;= D_RAM;
			else
			Y_RAM &lt;= Mem( conv_integer(A_ram) );
			end if;
		end if;
	end process;
 --Mem&lt;=c;

